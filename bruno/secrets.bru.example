# Bruno Secrets (DO NOT COMMIT)
# Copy this to secrets.bru and fill in your values

vars {
  # Production server token (NEVER commit this)
  prod_server_token: YOUR_PRODUCTION_SERVER_TOKEN_HERE
  
  # Production bearer token (JWT)
  prod_bearer_token: YOUR_PRODUCTION_JWT_HERE
  
  # Test player credentials
  test_guid: YOUR_TEST_PLAYER_GUID
  test_smf_id: YOUR_TEST_SMF_MEMBER_ID
  
  # Database admin credentials (for system endpoints)
  db_admin_token: YOUR_DB_ADMIN_TOKEN
  
  # CI/CD tokens
  ci_server_token: YOUR_CI_SERVER_TOKEN
  ci_bearer_token: YOUR_CI_BEARER_TOKEN
}

# Usage:
# 1. Copy this file to secrets.bru
# 2. Fill in actual values
# 3. Reference in requests: {{var:prod_server_token}}
# 4. secrets.bru is git-ignored automatically
